#, c-format
msgid "%s: Unexpected EOF"
msgstr "%s: Neatendita dosierfino"

#: src/pdb-lang.c:68
msgid "Unexpected tag in a ‘lingvo’ tag"
msgstr "Neatendita etikedo en etikedo ‘lingvo’"

#: src/pdb-man.c:115
#, c-format
msgid "Error starting groff: %s"
msgstr "Eraro startante groff: %s"

#: src/pdb-man.c:152
#, c-format
msgid "Error writing to groff: %s"
msgstr "Eraro skribante al groff: %s"

#: src/pdb-man.c:166
#, c-format
msgid "Error waiting for groff: %s"
msgstr "Eraro atendante groff: %s"

#: src/pdb-man.c:175
msgid "Failed to run groff"
msgstr "Malsukcesis ruli groff"

#: src/pdb-revo.c:143
msgid "Unzip failed"
msgstr "Unzip malsukcesis"

#: src/pdb-revo.c:382
msgid "Unexpected data from unzip"
msgstr "Neatendita datumo de unzip"

#: src/pdb-revo.c:399
msgid "Embedded '\\0' found in unzip listing"
msgstr "Enigita '\\0' troviĝis en la eligaĵo de unzip"

#: src/pdb-xml.c:71
#, c-format
msgid "Missing attribute “%s” on element “%s”"
msgstr "Mankas atributo “%s” al elemento “%s”"

#: src/pdb-xml.c:222
#, c-format
msgid "%s:%i:%i: An external entity was encountered without a system id"
msgstr "%s:%i:%i: Ekstera aĵo troviĝis kiu ne havas sisteman identigilon"

#: src/prevo.c:65
msgid "Location of the database file"
msgstr "Dosiernomo de la datumbazo"

#: src/prevo.c:69
msgid "Show completions for the word instead of an article"
msgstr "Montru kompletigojn de la vorto anstataŭ artikolon"

#: src/prevo.c:73
msgid "Show the raw nroff doc instead of piping it through man"
msgstr "Montru la krudan nroff-dokumenton anstataŭ ĉenigi ĝin tra man"

#: src/prevo.c:103
msgid "[language] <word>"
msgstr "[lingvo] <vorto>"

#: src/prevo.c:136
msgid "A word to lookup must be specified. See --help"
msgstr "Necesas specifi vorton por kontroli. Vidu --help"

#: src/prevo.c:325
#, c-format
msgid "The language “%s” was not found in the database"
msgstr "La lingvo “%s” ne troviĝis en la datumbazo"

#: src/prevo.c:349
#, c-format
msgid "%s: Invalid trie size"
msgstr "%s: Nevalida grandeco de la trie"

#: src/prevo.c:1030
#, c-format
msgid "%s: Invalid span"
msgstr "%s: Nevalida etendo"

#: src/prevo.c:1175
#, c-format
msgid "Index points to an invalid article number %i"
msgstr "La indekso indikas la nevalidan artikolnumeron %i"

#: src/prevo.c:1350
#, c-format
msgid "No article found for “%s”"
msgstr "Neniu artikolo troviĝis por “%s”"

#: src/prevo.c:1380
#, c-format
msgid "%s is not a PReVo database"
msgstr "%s ne estas datumbazo de PReVo"

#: src/prevo.c:1389
#, c-format
msgid "%s is using a database format that is too old"
msgstr "%s uzas datumbaz-formaton kiu estas tro malnova"

#: src/prevo.c:1398
#, c-format
msgid "%s is using an unknown database version"
msgstr "la versio de la datumbazo en %s estas nekonata"

#: src/prevo.c:1454
#, c-format
msgid ""
"No database file found. You can specify it directly with the -d option\n"
msgstr "Neniu datumbazo troviĝis. Vi povas specifi ĝin rekte per la opcio -d\n"

