#, c-format
msgid "sncref “%s” doesn’t point to a snc\n"
msgstr ""

#: src/pdb-db.c:1162
#, c-format
msgid "reference to section with title “%.*s” contains an invalid sncref\n"
msgstr ""

#: src/pdb-db.c:1528
msgid "<ref> tag found with a cel attribute"
msgstr "Etikedo <ref> troviĝis sen la atributo cel"

#: src/pdb-db.c:1587
#, fuzzy
msgid "<sncref> tag found with a target"
msgstr "Etikedo <ref> troviĝis sen la atributo cel"

#: src/pdb-db.c:2071
msgid "<drv> tag found with no <kap>"
msgstr "La etikedo <drv> troviĝis sen <kap>"

