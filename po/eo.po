msgid "Unexpected bare text in <subart> that has a <drv>"
msgstr "Ne atendita kruda teksto en <subart> kiu havas <drv>"

#: src/pdb-db.c:2293
msgid "<art> tag found with no <kap>"
msgstr "Etikedo <art> troviĝis sen <kap>"

#: src/pdb-db.c:2304
msgid "<kap> tag found with no <rad>"
msgstr "Etikedo <kap> troviĝis sen <rad>"

#: src/pdb-db.c:2571
#, c-format
msgid "no articles found in %s\n"
msgstr "neniuj artikoloj troviĝis en %s\n"

