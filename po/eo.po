#: ../src/main.c:35
msgid "Generate a single file instead of a db for Android"
msgstr "Generi unuopan dosieron anstataŭ datumbazon por Android"

#: ../src/main.c:39
msgid "The zip file or directory containing the ReVo XML files"
msgstr "La dosierujo aŭ zip-dosiero kiu enhavas la XML-dosierojn de ReVo"

#: ../src/main.c:43
msgid "Location for the output of the database"
msgstr "Dosiernomo por la rezulta datumbazo"

#: ../src/main.c:62
msgid "- Creates a compact database from the ReVo XML files"
msgstr "- Kreas kompaktan datumbazon el la XML-dosieroj de ReVo"

#: ../src/main.c:74 ../src/prevo.c:102
#, c-format
msgid "Unknown option '%s'"
msgstr "Nekonata opcio ‘%s’"

#: ../src/main.c:80
#, c-format
msgid "The -i and -o options are required. See --help"
msgstr "La opcioj -i kaj -o estas bezonataj. Vidu --help"

#: ../src/pdb-db.c:521
#, c-format
msgid "%s tag found with unknown parent %s"
msgstr "La etikedo %s troviĝis kun la nekonata patro %s"

#: ../src/pdb-db.c:534
#, c-format
msgid "drv node found without a kap"
msgstr "Nodo drv troviĝis sen nodo kap"

#: ../src/pdb-db.c:715 ../src/pdb-db.c:760
#, c-format
msgid "%s element with no lng attribute"
msgstr "Elemento %s troviĝis sen atributo lng"

#: ../src/pdb-db.c:982
#, c-format
msgid "using less precise mark \"%s\" for \"%s\"\n"
msgstr "uziĝas la malpli preciza marko “%s” por “%s”\n"

#: ../src/pdb-db.c:1002
#, c-format
msgid "no mark found for reference \"%s\"\n"
msgstr "neniu marko troviĝis por la referenco “%s”\n"

#: ../src/pdb-db.c:1315
#, c-format
msgid "<ref> tag found with a cel attribute"
msgstr "Etikedo <ref> troviĝis sen la atributo cel"

#: ../src/pdb-db.c:1781
#, c-format
msgid "<drv> tag found with no <kap>"
msgstr "La etikedo <drv> troviĝis sen <kap>"

#: ../src/pdb-db.c:1930
#, c-format
msgid "<%s> tag found in <subart> that has a <drv>"
msgstr "Etikedo <%s> troviĝis en <subart> kiu havas etikedon <drv>"

#: ../src/pdb-db.c:1948
#, c-format
msgid "Unexpected bare text in <subart> that has a <drv>"
msgstr "Ne atendita kruda teksto en <subart> kiu havas <drv>"

#: ../src/pdb-db.c:1996
#, c-format
msgid "<art> tag found with no <kap>"
msgstr "Etikedo <art> troviĝis sen <kap>"

#: ../src/pdb-db.c:2007
#, c-format
msgid "<kap> tag found with no <rad>"
msgstr "Etikedo <kap> troviĝis sen <rad>"

#: ../src/pdb-db.c:2076
#, c-format
msgid "no content found for article “%s”\n"
msgstr "Neniu enhavo troviĝis por la artikolo “%s”\n"

#: ../src/pdb-db.c:2270
#, c-format
msgid "no articles found in %s\n"
msgstr "neniuj artikoloj troviĝis en %s\n"

#: ../src/pdb-file.c:121
#, c-format
msgid "%s: Unexpected EOF"
msgstr "%s: Neatendita dosierfino"

#: ../src/pdb-lang.c:68
msgid "Unexpected tag in a ‘lingvo’ tag"
msgstr "Neatendita etikedo en etikedo ‘lingvo’"

#: ../src/pdb-man.c:115
#, c-format
msgid "Error starting groff: %s"
msgstr "Eraro startante groff: %s"

#: ../src/pdb-man.c:152
#, c-format
msgid "Error writing to groff: %s"
msgstr "Eraro skribante al groff: %s"

#: ../src/pdb-man.c:166
#, c-format
msgid "Error waiting for groff: %s"
msgstr "Eraro atendante groff: %s"

#: ../src/pdb-man.c:175
#, c-format
msgid "Failed to run groff"
msgstr "Malsukcesis ruli groff"

#: ../src/pdb-revo.c:143
msgid "Unzip failed"
msgstr "Unzip malsukcesis"

#: ../src/pdb-revo.c:382
#, c-format
msgid "Unexpected data from unzip"
msgstr "Neatendita datumo de unzip"

#: ../src/pdb-revo.c:399
msgid "Embedded '\\0' found in unzip listing"
msgstr "Enigita '\\0' troviĝis en la eligaĵo de unzip"

#: ../src/pdb-xml.c:71
#, c-format
msgid "Missing attribute “%s” on element “%s”"
msgstr "Mankas atributo “%s” al elemento “%s”"

#: ../src/pdb-xml.c:222
#, c-format
msgid "%s:%i:%i: An external entity was encountered without a system id"
msgstr "%s:%i:%i: Ekstera aĵo troviĝis kiu ne havas sisteman identigilon"

#: ../src/prevo.c:58
msgid "Location of the database file"
msgstr "Dosiernomo de la datumbazo"

#: ../src/prevo.c:62
msgid "Show completions for the word instead of an article"
msgstr "Montru kompletigojn de la vorto anstataŭ artikolon"

#: ../src/prevo.c:66
msgid "Show the raw nroff doc instead of piping it through man"
msgstr "Montru la krudan nroff-dokumenton anstataŭ ĉenigi ĝin tra man"

#: ../src/prevo.c:91
msgid "[language] <word>"
msgstr "[lingvo] <vorto>"

#: ../src/prevo.c:118
#, c-format
msgid "A word to lookup must be specified. See --help"
msgstr "Necesas specifi vorton por kontroli. Vidu --help"

#: ../src/prevo.c:307
#, c-format
msgid "The language “%s” was not found in the database"
msgstr "La lingvo “%s” ne troviĝis en la datumbazo"

#: ../src/prevo.c:331
#, c-format
msgid "%s: Invalid trie size"
msgstr "%s: Nevalida grandeco de la trie"

#: ../src/prevo.c:1012
#, c-format
msgid "%s: Invalid span"
msgstr "%s: Nevalida etendo"

#: ../src/prevo.c:1156
#, c-format
msgid "Index points to an invalid article number %i"
msgstr "La indekso indikas la nevalidan artikolnumeron %i"

#: ../src/prevo.c:1323
#, c-format
msgid "No article found for “%s”"
msgstr "Neniu artikolo troviĝis por “%s”"

#: ../src/prevo.c:1351
#, c-format
msgid "%s is not a PReVo database"
msgstr "%s ne estas datumbazo de PReVo"

#: ../src/prevo.c:1402
#, c-format
msgid ""
"No database file found. You can specify it directly with the -d option\n"
msgstr "Neniu datumbazo troviĝis. Vi povas specifi ĝin rekte per la opcio -d\n"
