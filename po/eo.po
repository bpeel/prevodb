#, c-format
msgid "%s element with no lng attribute"
msgstr "Elemento %s troviĝis sen atributo lng"

#: src/pdb-db.c:863
#, c-format
msgid "The language code “%s” is too long"
msgstr "La lingvokodo “%s” estas tro longa"

#: src/pdb-db.c:1096
#, c-format
msgid "using less precise mark \"%s\" for \"%s\"\n"
msgstr "uziĝas la malpli preciza marko “%s” por “%s”\n"

#: src/pdb-db.c:1118
#, c-format
msgid "no mark found for reference \"%s\"\n"
msgstr "neniu marko troviĝis por la referenco “%s”\n"

